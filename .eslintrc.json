{
  "extends": "next/core-web-vitals",
  "plugins": ["react-hooks"],
  "rules": {
    // "no-undef": "error",
    // "no-unused-vars": "warn",
    "no-duplicate-imports": "warn",
    // "import/no-unresolved": "error",
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": "off",
